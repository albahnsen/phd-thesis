@article{Breiman1998,
abstract = {Recent work has shown that combining multiple versions of unstable$\backslash$nclassifiers such as trees or neural nets results in reduced test$\backslash$nset error. One of the more effective is bagging. Here, modified training$\backslash$nsets are formed by resampling from the original training set, classifiers$\backslash$nconstructed using these training sets and then combined by voting.$\backslash$nFreund and Schapire propose an algorithm the basis of which is to$\backslash$nadaptively resample and combine (hence the acronym arcing) so that$\backslash$nthe weights in the resampling are increased for those cases most$\backslash$noften misclassified and the combining is done by weighted voting.$\backslash$nArcing is more successful than bagging in test set error reduction.$\backslash$nWe explore two arcing algorithms, compare them to each other and$\backslash$nto bagging, and try to understand how arcing works. We introduce$\backslash$nthe definitions of bias and variance for a classifier as components$\backslash$nof the test set error. Unstable classifiers can have low bias on$\backslash$na large range of data sets. Their problem is high variance. Combining$\backslash$nmultiple versions either through bagging or arcing reduces variance$\backslash$nsignificantly.},
author = {Breiman, Leo},
doi = {10.1214/aos/1024691079},
file = {:home/al/Mendeley/Breiman - 1998 - Arcing classifiers.pdf:pdf},
isbn = {00905364},
issn = {00905364},
journal = {Annals of Statistics},
keywords = {Bagging,Boosting,Decision trees,Ensemble methods,Error-correcting,Markov chain,Monte Carlo,Neural networks,Output coding},
pages = {801--849},
title = {{Arcing classifiers}},
volume = {26},
year = {1998}
}
